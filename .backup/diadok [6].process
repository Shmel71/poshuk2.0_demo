      <Content>&lt;ToolboxItemSettings DisplayName="Ожидать файл" InternalName="WaitFile" FriendlyName="Ожидает появление файла заданное время. Если файл за указанное время не будет обнаружен в папке, то сгенерируется ошибка" Width="200" Height="75" PathToImage="ExternalToolboxes\Image\ico_Files_Folders.png" PathToToolboxImage="{x:Null}" ImageType="LeftImage" NoDelete="False" Invisible="False" Proportional="True" Version="6" LinuxAvailable="True" Code="&amp;#xA;function WaitFile{{{block_id}}}&amp;#xA;{         &amp;#xA;    try&amp;#xA;    {&amp;#xA;        $__Error.Enter(&amp;quot;{{{block_native_id}}}&amp;quot;, &amp;quot;{{{ErrorLevel}}}&amp;quot;,&amp;quot;{{{ErrorDirection}}}&amp;quot;,&amp;quot;{{{ReportLevel}}}&amp;quot;,&amp;quot;{{{ErrorMessage}}}&amp;quot;);&amp;#xA;        $__Error.AddConnectors(&amp;quot;{{{connector-Error}}}&amp;quot;, &amp;quot;{{{connectorRight}}}&amp;quot;, &amp;quot;{{{connectorBottom}}}&amp;quot;); &amp;#xA;              &amp;#xA;        $DirectoryPath = {{{DirectoryPath}}}; &amp;#xA;        $FileName      = {{{FileName}}}; &amp;#xA;        $WaitTimeStr   = {{{WaitTime}}};&amp;#xA;    &amp;#xA;        $DirectoryPath = ExpandConstFilePath $DirectoryPath&amp;#xA;        &amp;#xA;        $_waittime = _toint($WaitTimeStr); &amp;#xA;                  &amp;#xA;        if($null -eq $FileName) { $FileName = &amp;quot;&amp;quot;; }&amp;#xA;        &amp;#xA;        $listfilename = New-Object Collections.Generic.List[string];  &amp;#xA;        $vartype = $FileName.GetType().Name;&amp;#xA;        if($vartype.Contains(&amp;quot;List&amp;quot;) -or $vartype.Contains(&amp;quot;[]&amp;quot;))&amp;#xA;        {        &amp;#xA;            foreach($s in $FileName)&amp;#xA;            {       &amp;#xA;                $_str = $s.ToString();&amp;#xA;                if($_str.Length -gt 0) { $listfilename.Add($_str); }&amp;#xA;            }&amp;#xA;        }&amp;#xA;        else&amp;#xA;        {&amp;#xA;            $_str = $FileName.ToString(); &amp;#xA;            if($_str.Length -gt 0) { $listfilename.Add($_str); }&amp;#xA;        }&amp;#xA;        &amp;#xA;        $stopwatch = [System.Diagnostics.Stopwatch]::StartNew();&amp;#xA;        while($true)&amp;#xA;        {        &amp;#xA;            $yeswait = $false;&amp;#xA;            foreach($itemfilename in $listfilename)&amp;#xA;            {&amp;#xA;                $list = [System.IO.Directory]::GetFiles($DirectoryPath, $itemfilename); &amp;#xA;                if($list.Length -gt 0)&amp;#xA;                {       &amp;#xA;                    $yeswait = $true;&amp;#xA;                    {{{Result}}} = $list[0];&amp;#xA;                    break;&amp;#xA;                }&amp;#xA;            }&amp;#xA;           &amp;#xA;            if($yeswait) { break; } &amp;#xA;            &amp;#xA;            [System.Threading.Thread]::Sleep(1000);&amp;#xA;            if($stopwatch.Elapsed.TotalSeconds -ge $_waittime) &amp;#xA;            {     &amp;#xA;                $stopwatch.Stop();&amp;#xA;                $__Error.Exception(&amp;quot;File Timeout Waiting error&amp;quot;);&amp;#xA;                return;&amp;#xA;            } &amp;#xA;        }&amp;#xA;        $stopwatch.Stop();&amp;#xA;    }&amp;#xA;    catch &amp;#xA;    { &amp;#xA;        $__Error.ExceptionCatch($_);&amp;#xA;        return;&amp;#xA;    }&amp;#xA;    $__Error.Quit();&amp;#xA;}&amp;#xA;" TextColor="#FFDADADA" ProtoName="" Language="" UrlVideo="{x:Null}" DerivedFrom="" NoResize="False" Container="False" MouseOverColor="Gray" MouseOverFill="Transparent" IsExpressionOnPause="False" ExpressionOnPause="" IsCollapsedBlock="False" Breakpoint="False" DirivedFrom="" xmlns="clr-namespace:ToolboxDesigner.Core;assembly=DiagramDesigner" xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:s="clr-namespace:System;assembly=mscorlib" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"&gt;&lt;ToolboxItemSettings.PathStyle&gt;&lt;av:Style TargetType="av:Path"&gt;&lt;av:Style.BasedOn&gt;&lt;av: